/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar9132.dtsi"

/ {
	compatible = "linksys,linksys-e2100l", "qca,ar9130";
	model = "Linksys E2100L";

	aliases {
		led-status = &system;
	};

	extosc: ref {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <40000000>;
	};

	keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 21 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		wifi {
			label = "qss";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};

	leds {
		compatible = "gpio-leds";
		power {
			label = "e2100l:blue:power";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
			default-state = "default-on";
		};

		system: wps_blue {
			label = "e2100l:blue:wps";
			gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
//			linux,default-trigger = "heartbeat";
		};

		wps_amber {
			label = "e2100l:amber:wps";
			gpios = <&gpio 9 GPIO_ACTIVE_LOW>;
		};

		wlan {
			label = "e2100l:blue:wlan";
			gpios = <&gpio 6 GPIO_ACTIVE_LOW>;
			default-state = "off";
			linux,default-trigger = "phy0tpt";
		};
	};

/*	rtl8366rb {
		compatible = "realtek,rtl8366rb";
		gpio-sda = <&gpio 18 GPIO_ACTIVE_HIGH>;
		gpio-sck = <&gpio 19 GPIO_ACTIVE_HIGH>;

		mdio-bus {
			status = "okay";

			phy0: ethernet-phy@0 {
				reg = <0>;
			};
		};
	};*/
};

&uart {
	status = "okay";
};

&pll {
	clocks = <&extosc>;
};

&usb {
	status = "okay";
};

&usb_phy {
	status = "okay";
};

&spi {
	status = "okay";
	num-cs = <1>;

	flash@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot: partition@0 {
				label = "u-boot";
				reg = <0x000000 0x040000>;
				read-only;
			};

			partition@40000 {
				label = "firmware";
				reg = <0x040000 0x7A0000>;
			};

			partition@7E0000 {
				label = "nvram";
				reg = <0x7E0000 0x010000>;
				read-only;
			};

			art: partition@7F0000 {
				label = "art";
				reg = <0x7F0000 0x010000>;
				read-only;
			};
		};
	};
};

&mdio0 {
	status = "okay";

	phy0: ethernet-phy@0 {
		reg = <0>;
		phy-mode = "rmii";
	};
};

&mdio1 {
	status = "okay";

	phy4: ethernet-phy@4 {
		reg = <4>;
		phy-mode = "rmii";
	};
};


&eth0 {
	status = "okay";

	phy-mode = "rmii";
	mtd-mac-address = <&uboot 0xe0000>;
	phy-handle = <&phy0>;

/*	fixed-link {
		speed = <1000>;
		full-duplex;
	};*/
};

&eth1 {
	status = "okay";

	phy-mode = "rmii";
	mtd-mac-address = <&uboot 0xe0000>;
	mtd-mac-address-increment = <1>;

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&wmac {
	status = "okay";
	mtd-cal-data = <&art 0x1000>;
	mtd-mac-address = <&uboot 0xe0000>;
};
